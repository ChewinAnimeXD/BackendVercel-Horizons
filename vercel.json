{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      },
        {
          "src": "/tasks",
          "dest": "https://horizons-front.vercel.app/homePage"
        }
    ]
}