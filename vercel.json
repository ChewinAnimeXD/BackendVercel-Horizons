{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
      "redirects": [
        {
          "source": "/tasks",
          "destination": "https://horizons-front.vercel.app/tasks"
        }
      ]
}